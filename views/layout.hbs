<body>

	<header>
		<div class="container">
			<div class="row">

				<div class="span3">
					<a href="/" title="Home"><img src="/assets/img/mm_logo_trans.png" alt="Mustache Monitor" /></a>
				</div>
				<div class="span9">

					<center>
						<h1>Share Your Stache
						
						{{#if app.req.session.user}}
							<a href="/user/logout" class="btn btn-info" title="Logout"><i class="icon-white icon-lock"></i> Logout</a>
						{{else}}
							<a href="/user/login" class="btn btn-info" title="Login"><i class="icon-white icon-user"></i> Login</a>
							<a href="/user/logout" class="btn btn-danger" title="Logout"><i class="icon-white icon-lock"></i> Logout</a>
						{{/if}}
						</h1>
						
								
						
					</center>
				</div>
				
			</div>
			<br/>


				<div class="navbar navbar-inverse hide hidden-desktop-disabled">
				  <div class="navbar-inner">
				    <div class="container">

						<a class="btn btn-navbar" data-toggle="collapse" data-target=".nav-collapse">
							<span class="icon-bar"></span>
							<span class="icon-bar"></span>
							<span class="icon-bar"></span>
						</a>

						<div class="row nav-collapse">
							<div class="span12">
								<ul class="nav">
									<li class="first"><a href="/" title="Home" >Home</a></li>
									<li class="last"><a href="/user" title="User" >User</a></li>
								</ul>	
							</div>
						</div>

				    </div>
				  </div>
				</div>


		</div>
	</header>

	<div id="content-wrapper">
		<div class="container">
		  <div class="row">
			    <div id="content" class="span12">
					{{{body}}}
			    </div>
			</div>
		</div>
	</div>


